<?xml version="1.0" encoding="utf-8"?>
<Project ToolsVersion="4.0" xmlns="http://schemas.microsoft.com/developer/msbuild/2003">
  <PropertyGroup>
    <OutDir>$(SolutionDir)bin\$(PlatformTarget)\$(Configuration)\</OutDir>
    <IntDir>$(SolutionDir)build\$(PlatformTarget)\apps\$(Configuration)\$(ProjectName)\</IntDir>
    <QtVarsOutputDir>$(SolutionDir)build\$(PlatformTarget)\apps\$(Configuration)\$(ProjectName)\qmake\</QtVarsOutputDir>
  </PropertyGroup>
  <ItemDefinitionGroup Condition="'$(Configuration)'=='Release'">
    <ClCompile>
      <PreprocessorDefinitions>QT_NO_DEBUG_OUTPUT</PreprocessorDefinitions>
    </ClCompile>
  </ItemDefinitionGroup>
</Project>